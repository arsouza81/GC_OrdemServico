        <img src="../imagens/logo-ufam-icet.png" alt="logo-ufam-icet" id="logo">
    </a>
    <!-- Menu de navegação -->
    <nav>
        <ul>
            <li><a href="/login">Gerencia</a></li>
        </ul>
    </nav>
</header>
